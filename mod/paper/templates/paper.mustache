<div class="paper-card">
    <div class="paper-name">{{name}}</div>
    <hr>
    <div class="info">
        <div class="item duration-div"><span class="">Duration: <b>{{duration}}</b></span></div>
        <div class="item date-div"><span class="">Date: <b>{{date}}</b></span></div>
        <div class="item date-div"><span class="">Last Date: <b>{{lastdate}}</b></span></div>
    </div>
    <div class="marks-div">
        <table id="markstable" class="table table-hover table-striped table-condensed">
            <thead>
                <tr>
                    <th>Question Type</th>
                    <th>Positive</th>
                    <th>Negative</th>
                </tr>
            </thead>
            <tbody>
                {{{markingscheme}}}
            </tbody>
        </table>
    </div>
    <div class="syllabus-div">
        <div style="text-align:center"><b>Syllabus</b></div>
        {{syllabus}}
    </div>
   

    <hr>
    <div class="action-div"> &nbsp;
        <div class="action show">
            <a href="{{testingportal_url}}" class="btn btn-primary" target="_blank">Show</a>
        </div>
    </div>


    <div id="performance">
        {{#performance}}
                <h3> {{name}} Analysis</h3>
                <table>
                        <tr>
                                <td>
                                    <table>
                                            <tr>
                                                <td>
                                                        <table class='cube table table-bordered table-responsive table-responsive table-condensed myTable' border=1  cellspacing=0 style="border:1px solid #97a1b2; width:300px; background:#fff;">
                                                            <tr class='headerStyle' style='font-weight: bold;color:white; background:#07889b;' valign="middle">
                                                                <td colspan="2"><p style='font-size:20px;font-weight: bold;color:white; text-align:center;'><center>Students Performance in  {{name}} </center></p></td>
                                                            </tr>
                                                            <tr>
                                                                <th>Total No. Of Questions</th>
                                                                <td>{{nques}}</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Correct</th>
                                                                <td>{{corr}}</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Wrong</th>
                                                                <td>{{ wrong }}</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Unattempted</th>
                                                                <td>{{ unattempt }}</td>
                                                            </tr>
                                                        </table>
                </td>
                                            </tr>
                                    </table>

                                </td>
                                <td>Chart</td>
                        </tr>
                </table>
                <table>
                        <tr>
                                <td>
                                    <table>
                                            <tr>
                                                <td>
                                                        <table class='cube table table-bordered table-responsive table-responsive table-condensed myTable' border=1  cellspacing=0 style="border:1px solid #97a1b2; width:300px; background:#fff;">
                                                            <tr class='headerStyle' style='font-weight: bold;color:white; background:#07889b;' valign="middle">
                                                                <td colspan="2"><p style='font-size:20px;font-weight: bold;color:white; text-align:center;'><center>{{name}} Analysis in Percentage</center></p></td>
                                                            </tr>
                                                            <tr>
                                                                <th>Correct</th>
                                                                <td>{{corr_percent}}%</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Wrong</th>
                                                                <td>{{wrong_percent}}%</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Unattempted</th>
                                                                <td>{{ unattempt_percent }}%</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Accuracy In Solving</th>
                                                                <td>{{ corr_accuracy }}%</td>
                                                            </tr>
                                                        </table>
                </td>
                                            </tr>
                                    </table>

                                </td>
                                <td>Chart</td>
                        </tr>
                </table>



                 <table>
                        <tr>
                                <td>
                                    <table>
                                            <tr>
                                                <td>
                                                         <table class='cube table table-bordered table-responsive table-responsive table-condensed myTable' border=1  cellspacing=0 style="border:1px solid #97a1b2; width:300px; background:#fff;">
                                                            <tr class='headerStyle' style='font-weight: bold;color:white; background:#07889b;' valign="middle">
                                                                <td colspan="2"><p style='font-size:20px;font-weight: bold;color:white; text-align:center;'><center>Marks Analysis - {{name}}</center></p></td>
                                                            </tr>
                                                            
                                                            <tr>
                                                                <th>Positive Marks</th>
                                                                <td>{{marks_correct}}</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Negative Marks</th>
                                                                <td>{{ negmarks }}</td>
                                                            </tr>
                                                            <tr>
                                                                <th>Marks Obtained</th>
                                                                <td>{{ obt }}</td>
                                                            </tr>
                                                        </table>
                </td>
                                            </tr>
                                    </table>

                                </td>
                                <td>Chart</td>
                        </tr>
                </table>


        {{/performance}}
        
           
     

    </div>
</div>
<style>
   .view_student_list_table_td_th {
   border: 1px solid #314c9a; 
   }
   .view_student_list_table {
   border-collapse: collapse;
   width: 100%; text-align: center;
   }
    .view_student_list_table_td_td {
   border: 1px solid #ccc; 
   }
   .view_bg{ background:#21409a; color:#fff;}
   h6{color:#21409a;}
     canvas {
        -moz-user-select: none;
        -webkit-user-select: none;
        -ms-user-select: none;
    }
    .cube { 
	-webkit-box-shadow: 0px 2px 13px 0px rgba(50, 50, 50, 0.76);
-moz-box-shadow:    0px 2px 13px 0px rgba(50, 50, 50, 0.76);
box-shadow:         0px 2px 13px 0px rgba(50, 50, 50, 0.76);
}
</style>


<link rel="stylesheet" href="templates/css/paper.css">
